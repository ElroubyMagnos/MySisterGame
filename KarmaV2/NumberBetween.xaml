<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KarmaV2.NumberBetween"
             Title="NumberBetween">
    <ScrollView>
        <VerticalStackLayout HorizontalOptions="Center">
            <Image WidthRequest="250" HeightRequest="250" x:Name="Minus" Source=""/>
            <Image WidthRequest="250" HeightRequest="250" x:Name="Plus" Source=""/>
            <HorizontalStackLayout>
                <Entry x:Name="String" WidthRequest="250" />
                <Button Text="Submit" Clicked="Button_Clicked" />
            </HorizontalStackLayout>
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>